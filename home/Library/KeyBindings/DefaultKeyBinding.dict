/* ~/Library/KeyBindings/DefaultKeyBinding.dict */
/* Credit: https://raw.githubusercontent.com/garybernhardt/dotfiles/master/Library/KeyBindings/DefaultKeyBinding.dict */
{
    "~f" = "moveWordForward:";
    "~b" = "moveWordBackward:";
    "~<" = "moveToBeginningOfDocument:";
    "~>" = "moveToEndOfDocument:";
    "~v" = "pageUp:";
    "~d" = "deleteWordForward:";
    "^w" = "deleteWordBackward:";
}
